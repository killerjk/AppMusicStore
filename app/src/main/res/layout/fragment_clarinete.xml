<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".tela_principal.fragments.ClarineteFragment">

    <com.google.android.material.appbar.AppBarLayout
        android:id="@+id/appbarlayout_pesquisar"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@color/green"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <androidx.appcompat.widget.Toolbar
                android:id="@+id/toolbar_pesquisar_clarinete"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/green"
                android:minHeight="?attr/actionBarSize"
                android:theme="?attr/actionBarTheme"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.0" />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </com.google.android.material.appbar.AppBarLayout>

    <View
        android:id="@+id/view_clarineteBb"
        android:layout_width="wrap_content"
        android:layout_height="100dp"
        android:layout_marginTop="70dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/image_clarineteBb"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:src="@drawable/clarinete_sib"
        app:layout_constraintBottom_toBottomOf="@id/view_clarineteBb"
        app:layout_constraintStart_toStartOf="@+id/view_clarineteBb"
        app:layout_constraintTop_toTopOf="@id/view_clarineteBb" />

    <TextView
        android:id="@+id/txt_clarineteBb"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:text="@string/clarineteBb"
        android:textAlignment="center"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@id/view_clarineteBb"
        app:layout_constraintEnd_toStartOf="@+id/veja_mais_clarineteBb"
        app:layout_constraintStart_toEndOf="@id/image_clarineteBb"
        app:layout_constraintTop_toTopOf="@+id/view_clarineteBb" />

    <Button
        android:id="@+id/veja_mais_clarineteBb"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="20dp"
        android:gravity="center"
        android:text="Veja \nMais"
        android:textColor="@color/black"
        app:backgroundTint="@color/green"
        app:layout_constraintBottom_toBottomOf="@id/view_clarineteBb"
        app:layout_constraintEnd_toEndOf="@id/view_clarineteBb"
        app:layout_constraintTop_toTopOf="@+id/view_clarineteBb" />

    <View
        android:id="@+id/view_clarineteEb"
        android:layout_width="wrap_content"
        android:layout_height="100dp"
        android:layout_marginTop="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/view_clarineteBb" />

    <ImageView
        android:id="@+id/image_clarineteEb"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:src="@drawable/clarinete_mib"
        app:layout_constraintBottom_toBottomOf="@id/view_clarineteEb"
        app:layout_constraintStart_toStartOf="@id/view_clarineteEb"
        app:layout_constraintTop_toTopOf="@id/view_clarineteEb" />

    <TextView
        android:id="@+id/txt_clarineteEb"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:text="@string/clarineteEb"
        android:textAlignment="center"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@id/view_clarineteEb"
        app:layout_constraintEnd_toStartOf="@+id/veja_mais_clarineteEb"
        app:layout_constraintStart_toEndOf="@id/image_clarineteEb"
        app:layout_constraintTop_toTopOf="@id/view_clarineteEb" />

    <Button
        android:id="@+id/veja_mais_clarineteEb"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="20dp"
        android:gravity="center"
        android:text="Veja \nMais"
        android:textColor="@color/black"
        app:backgroundTint="@color/green"
        app:layout_constraintBottom_toBottomOf="@id/view_clarineteEb"
        app:layout_constraintEnd_toEndOf="@id/view_clarineteEb"
        app:layout_constraintTop_toTopOf="@id/view_clarineteEb" />
</androidx.constraintlayout.widget.ConstraintLayout>